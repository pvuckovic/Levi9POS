{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich":  ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DESKTOP-E10ETIB\\SQLEXPRESS;Initial Catalog=POSTest;Integrated Security=True; TrustServerCertificate=True"
  },
  "JwtOptions": {
    "Issuer": "http://localhost:5067",
    "Audience": "http://localhost:5067",
    "SigningKey": "some-signing-key-here",
    "ExpirationSeconds": 86400
  }
}
