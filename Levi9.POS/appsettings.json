{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DESKTOP-887L9S3\\SQLEXPRESS;Initial Catalog=POSTest;Integrated Security=True;TrustServerCertificate=True"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Health Checks Dasboard",
        "Uri": "/healthcheck"
      }
    ],
    "EvaluationTimeInSeconds": 5
  },
  "JwtOptions": {
    "Issuer": "http://localhost:5067",
    "Audience": "http://localhost:5067",
    "SigningKey": "some-signing-key-here",
    "ExpirationSeconds": 86400
  }
}
